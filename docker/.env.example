# Enable/disable JWT authentication for non-trusted IPs
AUTHENTICATION_ENABLED=false

# JWT secret key for HS256 algorithm (required if AUTHENTICATION_ENABLED=true)
JWT_SECRET=your-secret-key-here

# Trusted IP addresses (comma-separated, supports multiple IPs)
# These IPs have full access (GET, POST, PUT, PATCH, DELETE) without JWT
# Examples: 127.0.0.1,172.18.0.1,10.0.0.5
TRUSTED_IPS=127.0.0.1,172.18.0.1
